NAME:
  create - Create a secret.

USAGE:
  create --name SECRET_NAME (--from-env-file FILE | --from-file FILE | --from-file KEY=FILE | --from-literal KEY=VALUE) [options...]

OPTIONS:
  --access-key-id             Specify the access_key_id of the HMAC credential.  
                              Aliases: --akid value  
                                
  --access-key-id-prompt      Prompt for the access_key_id of the HMAC credential. (default:  
                              false)  
                                
  --cert-chain-file           Specify a file containing the certificate chain provided by your  
                              certificate authority.  
                              Aliases: --ccf value  
                                
  --email                     The email address to access the registry server.  
                              Aliases: --em value  
                                
  --format                    Required. The format of the secret. Valid values are 'basic_auth',  
                              'generic', 'hmac', 'registry', 'ssh', or 'tls'. (default: "generic")  
                              Aliases: --fo value  
                                
  --from-env-file             Specify the path to a file to read lines of key=val pairs. This  
                              option can be specified multiple times.  (accepts multiple inputs)  
                              Aliases: -e value  
                                
  --from-file                 Set a key from a file. The format must be FILE or KEY=FILE. The  
                              entire contents of the file will be assigned to this one key. This  
                              option can be specified multiple times.  (accepts multiple inputs)  
                              Aliases: -f value  
                                
  --from-json-file            The path to a file containing the secret values in JSON format. This  
                              option is supported for secret format 'hmac'  
                              Aliases: --json value  
                                
  --from-literal              Set a key from a key-value pair. The format must be KEY=VALUE. This  
                              option can be specified multiple times.  (accepts multiple inputs)  
                              Aliases: -l value  
                                
  --key-path                  The path to your unencrypted SSH private key file.  
                              Aliases: --kp value  
                                
  --known-hosts-path          The path to your known_hosts file.  
                              Aliases: --khp value  
                                
  --name                      Required. The name of the secret. Use a name that is unique within  
                              the project.  
                              Aliases: -n value  
                                
  --output                    Output format. Valid values are 'json', 'yaml',  
                              'jsonpath=JSONPATH_EXPRESSION', and  
                              'jsonpath-as-json=JSONPATH_EXPRESSION'.  
                              Aliases: -o value  
                                
  --password                  The password for your basic_auth or registry secret.  
                              Aliases: --pw value  
                                
  --password-from-file        The path to a file containing the password. The first line of the  
                              file is used for the password. If neither 'password', nor  
                              'password-from-file', nor 'password-from-json-file' option is  
                              specified, then you are prompted for the password.  
                              Aliases: --spf value  
                                
  --password-from-json-file   The path to a JSON file containing the password. The 'apikey' field  
                              is used for the password. If neither 'password', nor  
                              'password-from-file', nor 'password-from-json-file' option is  
                              specified, then you are prompted for the password.  
                              Aliases: --spfj value  
                                
  --private-key-file          Specify a file containing the private key that matches the specified  
                              certificate chain with the 'cert-chain-file' option.  
                              Aliases: --pkf value  
                                
  --quiet                     Specify this option to reduce the output of the command. (default:  
                              false)  
                              Aliases: -q  
                                
  --secret-access-key         Specify the secret_access_key of the HMAC credential.  
                              Aliases: --sak value  
                                
  --secret-access-key-prompt  Prompt for the secret_access_key of the HMAC credential. (default:  
                              false)  
                                
  --server                    The URL of the registry server. (default: "us.icr.io")  
                              Aliases: -s value  
                                
  --username                  The username for your basic_auth or registry secret.  
                              Aliases: -u value  
                                
  --help                      Show help. (default: false)  
                              Aliases: -h  
                                
